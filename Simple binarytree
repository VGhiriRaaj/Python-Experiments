class Node:
    def __init__(self, data):
        self.left = None
        self.right = None
        self.data = data
        
        
    def PrintTree(self):
        print(self.data)
        
root = Node(1)
root.left = Node(2)
root.right = Node(3)
root.left.left = Node(4)
root.right.right = Node(5)
root.left.right = Node(6)
root.right.left = Node(7)
root.PrintTree()
