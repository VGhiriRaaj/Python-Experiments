class hashtable:
    def __init__(self):
        self.table = {}

    def insert(self, key, value):
        self.table[key] = value

    def search(self, key):
        return self.table.get(key, None)

    def delete(self, key):
        del self.table[key]


hash_table = hashtable()
hash_table.insert(1, 'delhi')
hash_table.insert(2, 'banglore')
hash_table.insert(3, 'chennai')
hash_table.insert(4, 'hyderabad')
hash_table.insert(5, 'Tirupati')

hash_table.delete(3)
print(hash_table.search(3))
print(hash_table.search(5))
